<Window x:Class="AttendanceEmp.LoginWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AttendanceEmp"
        mc:Ignorable="d"
        Title="LoginWindow" Height="444" Width="430">
    <Grid Margin="0,0,0,-6">
        <TextBox x:Name="LoginBox" HorizontalAlignment="Center" Height="33" Margin="0,134,0,0" TextWrapping="Wrap" Text="Логин" VerticalAlignment="Top" Width="226"/>
        <Button x:Name="LogButton" Content="Войти" HorizontalAlignment="Center" Height="30" Margin="0,239,0,0" VerticalAlignment="Top" Width="126" Click="LogButton_Click" Background="#FF92DCE5"/>
        <PasswordBox x:Name="PassBox" HorizontalAlignment="Center" Height="33" Margin="0,184,0,0" VerticalAlignment="Top" Width="226"/>

    </Grid>
</Window>
